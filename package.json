{
  "name": "sanity-convert-references",
  "version": "1.0.0",
  "description": "Reference optimization utility for Sanity Studio - convert strong to weak references for any document types with performance benefits",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "sanity",
    "sanity-studio",
    "references",
    "weak-references",
    "performance",
    "optimization",
    "utility",
    "admin-tool"
  ],
  "author": "Quinn Keaveney",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quitequinn/sanity-convert-references.git"
  },
  "bugs": {
    "url": "https://github.com/quitequinn/sanity-convert-references/issues"
  },
  "homepage": "https://github.com/quitequinn/sanity-convert-references#readme",
  "peerDependencies": {
    "@sanity/ui": "^1.0.0",
    "react": "^18.0.0",
    "sanity": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^29.0.0",
    "rollup": "^3.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@sanity/icons": "^2.0.0",
    "groq": "^3.0.0"
  }
}
